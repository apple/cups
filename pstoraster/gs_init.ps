	% Detect this by the disappearance of .delaybind.
   /bind
    { /.delaybind .systemvar dup length 0 ne
       { .delaycount 2 index put
         .userdict /.delaycount .delaycount 1 add put
       }
       { pop .bind
       }
      ifelse
